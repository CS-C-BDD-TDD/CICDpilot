<%- headers = ['Title', 'Description', 'Capec_ID', 'Created By', 'Date Created'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @attack_patterns.each do |atps| %>
  <%- row = [atps.title, atps.description, atps.capec_id, atps.created_by_user ? atps.created_by_user.username : '', atps.created_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, atps), :row_sep => '', :force_quotes => true %>
<% end %>
