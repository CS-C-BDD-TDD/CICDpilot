<%- headers = ['Name', 'Name Condition', 'Cybox Object ID', 'Created At', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @mutexes.each do |mutex| %>
  <%- row = [mutex.name, mutex.name_condition, mutex.cybox_object_id, mutex.created_at, mutex.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, mutex), :row_sep => '', :force_quotes => true %>
<% end %>
