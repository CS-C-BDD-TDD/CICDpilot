<%- headers = ['Cybox Object ID', 'Source Address', 'Source Hostname', 'Source Port', 'Destination Address', 'Destination Hostname', 'Destination Port', 'Layer 3 Protocol', 'Layer 4 Protocol', 'Layer 7 Protocol', 'Created At', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @network_connections.each do |connection| %>
  <%- row = [connection.cybox_object_id, connection.source_socket_address, connection.source_socket_hostname, connection.source_socket_port, connection.dest_socket_address, connection.dest_socket_hostname, connection.dest_socket_port, connection.layer3_protocol, connection.layer4_protocol, connection.layer7_protocol, connection.created_at, connection.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, connection), :row_sep => '', :force_quotes => true %>
<% end %>
