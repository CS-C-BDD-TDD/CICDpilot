<%- headers = ['Address Value', 'Address Condition', 'Created At', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @addresses.each do |address| %>
  <%- row = [address.address_value_normalized, address.address_condition, address.created_at, address.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, address), :row_sep => '', :force_quotes => true %>
<% end %>
