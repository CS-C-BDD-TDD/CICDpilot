<%- headers = ['URI', 'URI Condition', 'Created At', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<%- uriName = String.new %>
<% @uris.each do |uri| %>
  <% if uri.uri_normalized != nil then uriName = uri.uri_normalized else uriName = uri.uri_raw end %>
  <%- row = [uriName, uri.uri_condition, uri.created_at, uri.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, uri), :row_sep => '', :force_quotes => true %>
<% end %>
