<%- headers = ['Title', 'Created By', 'Short Description', 'Date Created', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @stix_packages.each do |package| %>
  <%- row = [package.title, package.created_by_user.username, package.short_description, package.created_at, package.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, package), :row_sep => '', :force_quotes => true %>
<% end %>
