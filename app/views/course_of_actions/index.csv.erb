<%- headers = ['Title', 'Description', 'Created at', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @course_of_actions.each do |coa| %>
  <%- row = [coa.title, coa.description, coa.created_at, coa.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, coa), :row_sep => '', :force_quotes => true %>
<% end %>
