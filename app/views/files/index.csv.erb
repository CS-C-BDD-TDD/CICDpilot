<%- headers = ['Display Name', 'File Name', 'File Name Condition', 'MD5', 'Created At', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @files.each do |file| %> 
  <%-row = [file.display_name, file.file_name, file.file_name_condition, file.md5, file.created_at, file.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, file), :row_sep => '', :force_quotes => true %>
<% end %>
