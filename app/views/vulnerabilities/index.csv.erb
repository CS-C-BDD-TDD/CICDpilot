<%- headers = ['Title', 'Description', 'Cve_id', 'osvdb_id', 'Created At', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<% @vulnerabilities.each do |vul| %>
  <%- row = [vul.title, vul.description, vul.cve_id, vul.osvdb_id, vul.created_at, vul.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, vul), :row_sep => '', :force_quotes => true %>
<% end %>
