<%- headers = ['Observable Value', 'Observable Type', 'Reference', 'FO Sets', 'Indicator Type', 'Title', 'Last Updated'] %>
<%= raw CSV.generate_line addPortionMarkingToCSV(headers), :row_sep => '', :force_quotes => true %>
<%- @indicators.each do |indicator| -%>
  <%- row = [indicator.observable_value, indicator.observable_type, indicator.reference, indicator.threat_actor_title, indicator.indicator_type, indicator.title, indicator.updated_at] -%>
<%= raw CSV.generate_line addPortionMarkingToCSV(nil, row, indicator), :row_sep => '', :force_quotes => true %>
<%- end -%>
