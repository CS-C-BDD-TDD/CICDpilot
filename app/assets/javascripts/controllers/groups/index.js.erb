app.controller('GroupsIndexController', ['$scope','$rootScope','Restangular',
  function ($scope, $rootScope,Restangular) {
    $rootScope.breadcrumbs = [{text:'Home',link:'#/',icon:'fa-home'},{text:'Groups',link:'#/groups',icon:'fa-group'}];
      $scope.fetching = true;
      Restangular.one("/groups").get().then(function(data){
      $scope.groups = data;
      $scope.fetching = false;
    });
}]);
