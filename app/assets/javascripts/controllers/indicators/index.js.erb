app.controller('IndicatorsIndexController', ['$scope','$rootScope','Restangular',
  function ($scope, $rootScope, Restangular) {
    $rootScope.breadcrumbs = [{text:'Home',link:'#/',icon:'fa-home'},{text:'All Indicators',link:'#/indicators',icon:'fa-thumb-tack'}];
    $scope.fetching = true;
    Restangular.one('indicators').get().then(function(data){
      $scope.indicators = data.indicators;
      $scope.metadata = data.metadata;
      $scope.fetching = false;
    },function(res){
      toastr.error("Unable to load Indicators");
      $scope.fetching = false;
    });
}]);
