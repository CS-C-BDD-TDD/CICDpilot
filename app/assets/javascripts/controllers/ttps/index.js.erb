app.controller('TtpsIndexController', ['$scope','$rootScope','Restangular',
  function ($scope, $rootScope, Restangular) {
    $rootScope.breadcrumbs = [{text:'Home',link:'#/',icon:'fa-home'},{text:'All TTPs',link:'#/ttps',icon:'fa-list'}];
    $scope.fetching = true;
    $scope.ttps = [];
    Restangular.one('ttps').get().then(function(data){ 
      $scope.ttps = data.ttps;
      $scope.metadata = data.metadata;
      $scope.fetching= false;
    },function(res){
      toastr.error("Unable to load TTPs");
      $scope.fetching= false;
    }); 
  }
]);
