app.controller('WeatherMapDomainsController', ['$scope','$rootScope','Restangular','paginationService',
  function ($scope, $rootScope, Restangular,paginationService) {
    $rootScope.breadcrumbs = [{text:'Home',link:'#/',icon:'fa-home'}];
    $rootScope.breadcrumbs = [{text:'Home',link:'#/',icon:'fa-home'},{text:'Weather Map Domains',link:'#/weathermapdomains',icon:'fa-thumb-tack'}];
    $scope.fetching = true;
    Restangular.one('/weather_map_domains').get().then(function(data){
      $scope.domains = data.domains;
      $scope.metadata = data.metadata;
      $scope.fetching = false;
    });
}]);
