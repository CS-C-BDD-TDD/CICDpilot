#!/bin/bash
CATALINA_HOME=${CATALINA_HOME:="/usr/share/tomcat7"}
CYBER_INDICATORS_HOME=${CYBER_INDICATORS_HOME:="/var/apps/cyber-indicators"}
KEYSTORE_PATH=${KEYSTORE_PATH:=$JAVA_HOME/jre/lib/security}

cp -prn $CATALINA_HOME/conf/* /etc/cyber-indicators/tomcat7/conf
rm -f $CATALINA_HOME/conf
ln -sf /etc/cyber-indicators/tomcat7/conf $CATALINA_HOME/conf
cp -prn $CATALINA_HOME/bin/* $CYBER_INDICATORS_HOME/tomcat7/bin/
rm -f $CYBER_INDICATORS_HOME/tomcat7/bin/*.bat
rm -f $CATALINA_HOME/bin
ln -sf $CYBER_INDICATORS_HOME/tomcat7/bin $CATALINA_HOME/bin
mkdir -p $CATALINA_HOME/keystore

