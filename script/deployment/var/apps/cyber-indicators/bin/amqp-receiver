# This file is automatically generated by the Cyber Indicators application
#
# DO NOT MODIFY THIS FILE
#!/bin/bash
# Get instance specific config file
if [ -r "/etc/sysconfig/cyber-indicators" ]; then
  set -a
  . /etc/sysconfig/cyber-indicators
  set +a
fi

if [ -r "/etc/sysconfig/amqp-receiver" ]; then
  set -a
  . /etc/sysconfig/amqp-receiver
  set +a
fi

export CATALINA_HOME=${CATALINA_HOME:="/usr/share/tomcat7"}
export TNS_ADMIN=${TNS_ADMIN:="/etc"}
export WAR_FILE=${WAR_FILE:="$CATALINA_HOME/webapps/cyber-indicators.war"}

exec $JAVA_HOME/bin/java $JAVA_OPTS -cp "$AMQP_RECEIVER_JAR_PATH/*:$WAR_FILE" WarMain -S rake amqp_receiver:run
