#!/bin/bash
echo "Initializing database configuration..."
touch /etc/cyber-indicators/config/database.yml
DATABASE=$DATABASE DBADMIN_USERNAME=$DBADMIN_USERNAME DBADMIN_PASSWORD=$DBADMIN_PASSWORD APPUSER_USERNAME=$APPUSER_USERNAME APPUSER_PASSWORD=$APPUSER_PASSWORD OUTFILE=/etc/cyber-indicators/config/database.yml TEMPLATE=/etc/cyber-indicators/templates/etc/cyber-indicators/config/database.yml.erb /var/apps/cyber-indicators/bin/rake db:template:create
